    "css-loader": "^6.8.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.1",
    "file-loader": "^6.2.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "needle": "^3.2.0",
    "pexels": "^1.4.0",
    "pg": "^8.8.0",
    "react-image-crop": "^11.0.1",
    "react-image-file-resizer": "^0.4.8",
    "react-router-dom": "^6.16.0",
    "react-simple-star-rating": "^5.1.7",
    "reselect": "^4.1.8",
