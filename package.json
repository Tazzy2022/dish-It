{
  "name": "dish",
  "version": "2.0.0",
  "description": "app using yelp fusion api to personalize restaurant lists",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "build:dev": "npm run build -- --watch --mode=development",
    "start": "node server/index.js --ignore public/ --ignore client/",
    "start:dev": "npm run build:dev & npm run start-server",
